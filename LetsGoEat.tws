V/%\u000aFor loop that picks random hates and preferences for the friends.\u000a%/\u000a<<if $i lte $friends.length-1>>\u000a<<set $restaurantGenres = ["Pizza", "Steak", "Vegetarian", "Thai", "Indian", "Mexican", "Burgers"];\u000a$prefpick = Math.floor(Math.random()*(\u000a$restaurantGenres.length));\u000a$hatepick = Math.floor(Math.random()*(\u000a$restaurantGenres.length));\u000a>>\u000a<<if $prefpick eq $hatepick>>\u000a<<if $prefpick eq 0>>\u000a<<set $hatepick = $prefpick + Math.floor(Math.random()*(\u000a$restaurantGenres.length)+1)>>\u000a<<else>>\u000a<<set $hatepick = $prefpick - 1>>\u000a<<endif>>\u000a<<endif>>\u000a<<set $friends[$i].secretpref = $restaurantGenres[$prefpick]>>\u000a<<set $friends[$i].secrethate = $restaurantGenres[$hatepick]>>\u000a/%<<print $friends[$i].name>>\u000a<<print $friends[$i].hungerlevel>>\u000a<<print $friends[$i].pickiness>>\u000a<<print $friends[$i].money>>\u000a<<print $friends[$i].secretpref>>\u000a<<print $friends[$i].secrethate>>%/\u000a<<set $i = $i + 1>>\u000a<<display Stats Roll Up For Loop>>\u000a<<endif>>
